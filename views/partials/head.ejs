     <!-- Google Translate Element -->
     <script type="text/javascript">
          function googleTranslateElementInit() {
               new google.translate.TranslateElement({
                    pageLanguage: 'en',
                    includedLanguages: 'en,fr,es,it,ar,ja,zh,pl',
                    layout: google.translate.TranslateElement.InlineLayout.SIMPLE,
                    autoDisplay: false,
                    multilanguagePage: true,
                    gaTrack: false,
                    gaId: ''
               }, 'google_translate_element');

               // Add toggle button for Google Translate navbar
               const header = document.querySelector('header');
               const toggleButton = document.createElement('button');
               toggleButton.id = 'translate-toggle';
               toggleButton.className = 'fixed top-4 right-4 z-50 bg-orange-500 hover:bg-orange-600 text-white rounded-full p-2 shadow-lg transition-all duration-300 transform hover:scale-105';
               toggleButton.innerHTML = '<i class="fas fa-language text-lg"></i>';
               toggleButton.style.display = 'none'; // Initially hidden
               document.body.appendChild(toggleButton);

               // Handle toggle button click
               toggleButton.addEventListener('click', () => {
                    const frame = document.querySelector('.goog-te-banner-frame');
                    if (frame) {
                         if (frame.style.display === 'none') {
                              frame.style.display = 'block';
                              toggleButton.classList.add('active');
                         } else {
                              frame.style.display = 'none';
                              toggleButton.classList.remove('active');
                         }
                    }
               });

               // Function to close the language modal
               function closeLanguageModal() {
                    // Find all elements with x-data that might contain languageModal
                    document.querySelectorAll('[x-data]').forEach(el => {
                         if (el.__x) {
                              // Try to set languageModal to false
                              try {
                                   el.__x.$data.languageModal = false;
                              } catch (e) {
                                   // Ignore if the element doesn't have languageModal
                              }
                         }
                    });

                    // Also try to find and click any close button in the modal
                    const closeButton = document.querySelector('[x-show="languageModal"] button[type="button"]');
                    if (closeButton) {
                         closeButton.click();
                    }
               }

               // Show toggle button when translation is active
               const observer = new MutationObserver((mutations) => {
                    mutations.forEach(mutation => {
                         if (mutation.type === 'childList' && mutation.addedNodes.length > 0) {
                              // Check for Google Translate elements
                              const frame = document.querySelector('.goog-te-banner-frame');
                              if (frame) {
                                   frame.style.display = 'none';
                                   toggleButton.style.display = 'block';
                              }

                              // Add globe icon and enhance dropdown
                              const menuValue = document.querySelector('.goog-te-menu-value');
                              if (menuValue && !menuValue.querySelector('.fa-globe')) {
                                   const globeIcon = document.createElement('i');
                                   globeIcon.className = 'fas fa-globe text-[#3e3065] mr-2';
                                   menuValue.insertBefore(globeIcon, menuValue.firstChild);

                                   // Add custom dropdown arrow
                                   const arrowIcon = document.createElement('i');
                                   arrowIcon.className = 'fas fa-chevron-down text-[#3e3065] ml-2 transition-transform duration-300';
                                   menuValue.appendChild(arrowIcon);
                              }

                              // Clean up any extra elements
                              const gadget = document.querySelector('.goog-te-gadget');
                              if (gadget) {
                                   Array.from(gadget.childNodes).forEach(node => {
                                        if (node.nodeType === 3) {
                                             node.textContent = '';
                                        }
                                   });
                              }

                              // Handle language selection
                              const languageSelect = document.querySelector('.goog-te-combo');
                              if (languageSelect && !languageSelect.dataset.eventAttached) {
                                   languageSelect.dataset.eventAttached = 'true';
                                   languageSelect.addEventListener('change', () => {
                                        setTimeout(closeLanguageModal, 50);
                                   });
                              }

                              // Handle menu item clicks
                              const menuItems = document.querySelectorAll('.goog-te-menu2-item');
                              menuItems.forEach(item => {
                                   if (!item.dataset.eventAttached) {
                                        item.dataset.eventAttached = 'true';
                                        item.addEventListener('click', () => {
                                             setTimeout(closeLanguageModal, 50);
                                        });
                                   }
                              });
                         }
                    });
               });

               observer.observe(document.body, {
                    childList: true,
                    subtree: true,
                    attributes: true,
                    characterData: true
               });

               // Add event listener for the Google Translate callback
               if (window.google && window.google.translate) {
                    window.google.translate.TranslateService.getInstance().addEventListener('translate', function() {
                         closeLanguageModal();
                    });
               }
          }
     </script>
