     <style>
          :root {
               --primary-color: #2563eb;
               --secondary-color: #0f172a;
               --accent-color: #ff6b00;
               --bg-blur: 10px;
               --transition-normal: 0.3s ease;
               --transition-slow: 0.5s ease;
               --shadow-sm: 0 1px 2px rgba(0,0,0,0.1);
               --shadow-md: 0 4px 6px -1px rgba(0,0,0,0.1), 0 2px 4px -1px rgba(0,0,0,0.06);
               --shadow-lg: 0 10px 15px -3px rgba(0,0,0,0.1), 0 4px 6px -2px rgba(0,0,0,0.05);
               --shadow-xl: 0 20px 25px -5px rgba(0,0,0,0.1), 0 10px 10px -5px rgba(0,0,0,0.04);
               --border-radius-sm: 0.25rem;
               --border-radius-md: 0.5rem;
               --border-radius-lg: 0.75rem;
               --border-radius-xl: 1rem;
               --border-radius-full: 9999px;
          }

          /* Apply heading font to all headings */
          h1, h2, h3, h4, h5, h6 {
               font-family: 'Montserrat', sans-serif;
               letter-spacing: -0.025em;
          }

          /* Apply body font to all text content */
          body {
               font-family: 'Poppins', sans-serif;
               overflow-x: hidden;
               scroll-behavior: smooth;
          }

          /* Glass effects */
          .glass {
               background: rgba(255, 255, 255, 0.8);
               backdrop-filter: blur(var(--bg-blur));
               -webkit-backdrop-filter: blur(var(--bg-blur));
               border: 1px solid rgba(255, 255, 255, 0.18);
               box-shadow: var(--shadow-md);
          }
          
          .glass-dark {
               background: rgba(15, 23, 42, 0.8);
               backdrop-filter: blur(var(--bg-blur));
               -webkit-backdrop-filter: blur(var(--bg-blur));
               border: 1px solid rgba(255, 255, 255, 0.08);
               box-shadow: var(--shadow-md);
          }

          /* Enhanced buttons */
          .btn-primary {
               background-image: linear-gradient(135deg, #ff6b00, #ea580c);
               color: white;
               padding: 0.75rem 1.5rem;
               border-radius: var(--border-radius-full);
               font-weight: 600;
               transition: transform var(--transition-normal), box-shadow var(--transition-normal);
               box-shadow: 0 4px 14px rgba(234, 88, 12, 0.4);
          }
          
          .btn-primary:hover {
               transform: translateY(-2px);
               box-shadow: 0 6px 20px rgba(234, 88, 12, 0.5);
          }
          
          .btn-primary:active {
               transform: translateY(1px);
          }

          /* Card styling */
          .card {
               background: white;
               border-radius: var(--border-radius-lg);
               overflow: hidden;
               transition: transform var(--transition-normal), box-shadow var(--transition-normal);
               box-shadow: var(--shadow-md);
          }
          
          .card:hover {
               transform: translateY(-5px);
               box-shadow: var(--shadow-xl);
          }

          /* Custom scrollbar for browsers that support it */
          ::-webkit-scrollbar {
               width: 8px;
               height: 8px;
          }

          ::-webkit-scrollbar-track {
               background: #f1f1f1;
               border-radius: 4px;
          }

          ::-webkit-scrollbar-thumb {
               background: #ea580c;
               border-radius: 4px;
          }

          ::-webkit-scrollbar-thumb:hover {
               background: #c2410c;
          }

          /* Animate headings */
          .animate-gradient-text {
               background-image: linear-gradient(90deg, #ea580c, #ff6b00, #ea580c);
               background-size: 200% auto;
               color: transparent;
               -webkit-background-clip: text;
               background-clip: text;
               animation: gradient-animation 6s linear infinite;
          }
          
          @keyframes gradient-animation {
               0% { background-position: 0% center; }
               100% { background-position: 200% center; }
          }
          
          /* Loading dots animation */
          .loading-dots::after {
               content: '';
               animation: loading-dots 1.5s infinite;
          }
          
          @keyframes loading-dots {
               0% { content: '.'; }
               33% { content: '..'; }
               66% { content: '...'; }
          }
          
          /* Enhanced focus styles */
          *:focus-visible {
               outline: 2px solid #ea580c;
               outline-offset: 2px;
          }
          
          /* Mobile menu animations */
          @keyframes fadeIn {
               from { opacity: 0; transform: translateY(-10px); }
               to { opacity: 1; transform: translateY(0); }
          }
          
          .fade-in {
               animation: fadeIn 0.3s ease forwards;
          }
     </style>

