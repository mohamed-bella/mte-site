     <!-- Custom GTranslate Implementation -->
     <script type="text/javascript">
     function doGTranslate(lang_pair) {
          if (lang_pair.value) lang_pair = lang_pair.value;
          if (lang_pair == '') return;
          var lang = lang_pair.split('|')[1];
          // Create a cookie
          var now = new Date();
          var expTime = now.getTime() + 1000 * 3600 * 24 * 365;
          now.setTime(expTime);
          document.cookie = 'googtrans=/'+lang_pair.split('|')[0]+'/'+lang_pair.split('|')[1]+'; expires='+now.toGMTString()+'; path=/';
          document.cookie = 'googtrans=/'+lang_pair.split('|')[0]+'/'+lang_pair.split('|')[1]+'; expires='+now.toGMTString()+'; path=/'+location.hostname;
          
          // Force reload to apply translation
          location.reload();
     }
     </script>
